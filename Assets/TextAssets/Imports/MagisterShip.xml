    <ID>HN2_DEVICE</ID>
    <Name>$HN2LogEndName</Name>
    <Curiosity>HN2_DEVICE</Curiosity>
    <IsCuriosity/>

    <RumorFact>
      <ID>HN2_Device_RumorChamber</ID>
      <SourceID>HN2_GC</SourceID>
      <RumorName>$HN2LogEndNameRumor</RumorName>
      <Text>$HN2LogEndRumorChamber</Text>
    </RumorFact>

    <RumorFact>
      <ID>HN2_Device_RumorLibrary</ID>
      <SourceID>HN2_LB</SourceID>
      <RumorName>$HN2LogEndNameRumor</RumorName>
      <Text>$HN2LogEndRumorLibrary</Text>
    </RumorFact>

    <RumorFact>
      <ID>HN2_Device_RumorEngine</ID>
      <SourceID>HN2_EG</SourceID>
      <RumorName>$HN2LogEndNameRumor</RumorName>
      <Text>$HN2LogEndRumorEngine</Text>
    </RumorFact>

    <RumorFact>
      <ID>HN2_Device_RumorMagi</ID>
      <SourceID>HN2_MG</SourceID>
      <RumorName>$HN2LogEndNameRumor</RumorName>
      <Text>$HN2LogEndRumorMagi</Text>
    </RumorFact>

    <ExploreFact>
      <ID>HN2_Device1</ID>
      <Text>$HN2LogEnd1</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN2_Device2</ID>
      <Text>$HN2LogEnd2</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN2_Device3</ID>
      <Text>$HN2LogEnd3</Text>
    </ExploreFact>
  </Entry>
  
