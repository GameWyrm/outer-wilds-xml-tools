    <ExploreFact>
      <ID>HN2_DM1</ID>
      <Text>$HN2LogDorms1</Text>
    </ExploreFact>
  
    <ExploreFact>
      <ID>HN2_DM2</ID>
      <Text>$HN2LogDorms2</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN2_DM3</ID>
      <Text>$HN2LogDorms3</Text>
    </ExploreFact>

    <ExploreFact>
