    <Curiosity>HN2_DEVICE</Curiosity>

    <ExploreFact>
      <ID>HN2_HN1</ID>
      <IgnoreMoreToExplore/>
      <Text>$HN2LogHN1</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN2_HN2</ID>
      <IgnoreMoreToExplore/>
      <Text>$HN2LogHN2</Text>
    </ExploreFact>
  </Entry>

  <!--Intro-->
